    "def smooth_weighted_avg (arr):\n",
    "    points = arr[-5:]\n",
    "    points[2] = 2 * points[2]\n",
    "    points[3] = 3 * points[3]\n",
    "    points[4] = 3 * points[4]\n",
    "    return (sum(points))/10"
   ]
  },
